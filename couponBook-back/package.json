{
  "name": "coupon-book-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon index.js",
    "start": "node index.js",
    "predev": "sequelize-cli db:create; sequelize-cli db:migrate",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:migrate": "sequelize-cli db:migrate",
    "db:reset": "sequelize-cli db:drop && npm db:create && npm db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:create": "sequelize-cli migration:generate --name && npm run format:migrations",
    "db:seed:all": "sequelize-cli db:seed:all",
    "seed:create": "sequelize-cli seed:create --name",
    "db:seed:undo": "sequelize-cli db:seed:undo --seed",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "format:migrations": "prettier --write migrations/*.js",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-list-routes": "^1.3.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
